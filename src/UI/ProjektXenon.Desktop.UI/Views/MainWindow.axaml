<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:ProjektXenon.Shared.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="using:ProjektXenon.Desktop.UI.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="ProjektXenon.Desktop.UI.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        MinWidth="895"
        MinHeight="835"
		ExtendClientAreaToDecorationsHint="True"
		ExtendClientAreaTitleBarHeightHint="54"
        Title="ProjektXenon">
	<Window.Styles>
		<Style Selector="Window /template/ CaptionButtons">
			<Setter Property="Margin" Value="30,20"/>
		</Style>
		<Style Selector="Window /template/ CaptionButtons Button.PART_FullScreenButton">
			<Setter Property="IsVisible" Value="True"/>
		</Style>
	</Window.Styles>
        <views:MainView/>
</Window>
